<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="paletteRoot"
      alignment="TOP_CENTER"
      managed="false"
      maxHeight="-Infinity"
      maxWidth="520.0"
      prefWidth="520.0"
      spacing="8.0"
      styleClass="palette-root"
      visible="false"
      xmlns="http://javafx.com/javafx/21"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.chaiware.acommander.palette.CommandPaletteController">
    <padding>
        <Insets bottom="6.0" left="6.0" right="6.0" top="6.0"/>
    </padding>
    <TextField fx:id="queryField"
               promptText="Type a command..."
               styleClass="palette-query"/>
    <ListView fx:id="resultsList"
              styleClass="palette-results"/>
</VBox>
