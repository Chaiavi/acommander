<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<StackPane xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.chaiware.acommander.Commander">
<BorderPane fx:id="rootPane" prefHeight="600.0" prefWidth="900.0" style="-fx-background-color: #0000AA;">
    <center>
        <GridPane hgap="2" style="-fx-background-color: #0000AA;" vgap="2">
            <columnConstraints>
                <ColumnConstraints percentWidth="50.0" />
                <ColumnConstraints percentWidth="50.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="35.0" vgrow="NEVER" />
                <RowConstraints minHeight="25.0" vgrow="NEVER" />
                <RowConstraints minHeight="200.0" vgrow="ALWAYS" />
            </rowConstraints>

            <!-- Drive/Path ComboBoxes -->
            <ComboBox fx:id="leftPathComboBox" editable="true" maxWidth="Infinity" prefHeight="30.0" style="-fx-background-color: #FFFFFF; -fx-text-fill: #000000; -fx-border-color: #000000;" GridPane.columnIndex="0" GridPane.hgrow="ALWAYS" GridPane.rowIndex="0">
                <GridPane.margin>
                    <Insets left="3.0" right="1.0" top="3.0" />
                </GridPane.margin>
            </ComboBox>

            <ComboBox fx:id="rightPathComboBox" editable="true" maxWidth="Infinity" prefHeight="30.0" style="-fx-background-color: #FFFFFF; -fx-text-fill: #000000; -fx-border-color: #000000;" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="0">
                <GridPane.margin>
                    <Insets left="1.0" right="3.0" top="3.0" />
                </GridPane.margin>
            </ComboBox>

            <!-- File Lists -->
            <ListView fx:id="leftFileList" prefHeight="200.0" style="-fx-background-color: #0000AA; -fx-control-inner-background: #0000AA; -fx-text-fill: #FFFF00; -fx-border-color: #FFFFFF; -fx-border-width: 1; -fx-font-family: 'Courier New', monospace; -fx-font-size: 12px;" GridPane.columnIndex="0" GridPane.rowIndex="1" GridPane.rowSpan="2">
                <GridPane.margin>
                    <Insets bottom="2.0" left="3.0" right="1.0" />
                </GridPane.margin>
            </ListView>

            <ListView fx:id="rightFileList" prefHeight="200.0" style="-fx-background-color: #0000AA; -fx-control-inner-background: #0000AA; -fx-text-fill: #FFFF00; -fx-border-color: #FFFFFF; -fx-border-width: 1; -fx-font-family: 'Courier New', monospace; -fx-font-size: 12px;" GridPane.columnIndex="1" GridPane.rowIndex="1" GridPane.rowSpan="2">
                <GridPane.margin>
                    <Insets bottom="2.0" left="1.0" right="3.0" />
                </GridPane.margin>
            </ListView>
        </GridPane>
    </center>

    <bottom>
        <!-- Function keys bar -->
        <VBox style="-fx-background-color: #C0C0C0; -fx-border-color: #808080; -fx-border-width: 1 0 0 0;">
            <HBox alignment="CENTER" prefHeight="40.0" spacing="1" style="-fx-background-color: #C0C0C0; -fx-padding: 3;">
                <Button fx:id="btnF1" maxWidth="Infinity" onAction="#help" prefHeight="32.0" style="-fx-background-color: #E0E0E0; -fx-text-fill: #000000; -fx-font-size: 10px; -fx-font-weight: bold; -fx-border-color: #808080; -fx-border-width: 1; -fx-background-radius: 3; -fx-border-radius: 3;" HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets left="5.0" />
                    </HBox.margin></Button>
                <Button fx:id="btnF2" maxWidth="Infinity" onAction="#renameFile" prefHeight="32.0" style="-fx-background-color: #E0E0E0; -fx-text-fill: #000000; -fx-font-size: 10px; -fx-font-weight: bold; -fx-border-color: #808080; -fx-border-width: 1; -fx-background-radius: 3; -fx-border-radius: 3;" HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets left="5.0" />
                    </HBox.margin></Button>
                <Button fx:id="btnF3" maxWidth="Infinity" onAction="#viewFile" prefHeight="32.0" style="-fx-background-color: #E0E0E0; -fx-text-fill: #000000; -fx-font-size: 10px; -fx-font-weight: bold; -fx-border-color: #808080; -fx-border-width: 1; -fx-background-radius: 3; -fx-border-radius: 3;" HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets left="5.0" />
                    </HBox.margin></Button>
                <Button fx:id="btnF4" maxWidth="Infinity" onAction="#editFile" prefHeight="32.0" style="-fx-background-color: #E0E0E0; -fx-text-fill: #000000; -fx-font-size: 10px; -fx-font-weight: bold; -fx-border-color: #808080; -fx-border-width: 1; -fx-background-radius: 3; -fx-border-radius: 3;" HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets left="5.0" />
                    </HBox.margin></Button>
                <Button fx:id="btnF5" maxWidth="Infinity" onAction="#copyFile" prefHeight="32.0" style="-fx-background-color: #E0E0E0; -fx-text-fill: #000000; -fx-font-size: 10px; -fx-font-weight: bold; -fx-border-color: #808080; -fx-border-width: 1; -fx-background-radius: 3; -fx-border-radius: 3;" HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets left="5.0" />
                    </HBox.margin></Button>
                <Button fx:id="btnF6" maxWidth="Infinity" onAction="#moveFile" prefHeight="32.0" style="-fx-background-color: #E0E0E0; -fx-text-fill: #000000; -fx-font-size: 10px; -fx-font-weight: bold; -fx-border-color: #808080; -fx-border-width: 1; -fx-background-radius: 3; -fx-border-radius: 3;" HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets left="5.0" />
                    </HBox.margin></Button>
                <Button fx:id="btnF7" maxWidth="Infinity" onAction="#makeDirectory" prefHeight="32.0" style="-fx-background-color: #E0E0E0; -fx-text-fill: #000000; -fx-font-size: 10px; -fx-font-weight: bold; -fx-border-color: #808080; -fx-border-width: 1; -fx-background-radius: 3; -fx-border-radius: 3;" HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets left="5.0" />
                    </HBox.margin></Button>
                <Button fx:id="btnF8" maxWidth="Infinity" onAction="#deleteFile" prefHeight="32.0" style="-fx-background-color: #E0E0E0; -fx-text-fill: #000000; -fx-font-size: 10px; -fx-font-weight: bold; -fx-border-color: #808080; -fx-border-width: 1; -fx-background-radius: 3; -fx-border-radius: 3;" HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets left="5.0" />
                    </HBox.margin></Button>
                <Button fx:id="btnF9" maxWidth="Infinity" onAction="#terminalHere" prefHeight="32.0" style="-fx-background-color: #E0E0E0; -fx-text-fill: #000000; -fx-font-size: 10px; -fx-font-weight: bold; -fx-border-color: #808080; -fx-border-width: 1; -fx-background-radius: 3; -fx-border-radius: 3;" HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets left="5.0" />
                    </HBox.margin></Button>
                <Button fx:id="btnF10" maxWidth="Infinity" onAction="#search" prefHeight="32.0" style="-fx-background-color: #E0E0E0; -fx-text-fill: #000000; -fx-font-size: 10px; -fx-font-weight: bold; -fx-border-color: #808080; -fx-border-width: 1; -fx-background-radius: 3; -fx-border-radius: 3;" HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets left="5.0" />
                    </HBox.margin></Button>
                <Button fx:id="btnF11" maxWidth="Infinity" onAction="#pack" prefHeight="32.0" style="-fx-background-color: #E0E0E0; -fx-text-fill: #000000; -fx-font-size: 10px; -fx-font-weight: bold; -fx-border-color: #808080; -fx-border-width: 1; -fx-background-radius: 3; -fx-border-radius: 3;" HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets left="5.0" />
                    </HBox.margin></Button>
                <Button fx:id="btnF12" maxWidth="Infinity" onAction="#unpackFile" prefHeight="32.0" style="-fx-background-color: #E0E0E0; -fx-text-fill: #000000; -fx-font-size: 10px; -fx-font-weight: bold; -fx-border-color: #808080; -fx-border-width: 1; -fx-background-radius: 3; -fx-border-radius: 3;" HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets left="5.0" right="5.0" />
                    </HBox.margin></Button>
            </HBox>
        </VBox>
    </bottom>
</BorderPane>
    <fx:include fx:id="commandPalette" source="CommandPalette.fxml" StackPane.alignment="TOP_CENTER">
        <StackPane.margin>
            <Insets top="42.0"/>
        </StackPane.margin>
    </fx:include>
</StackPane>
