{
  "actions": [
    {
      "id": "multiRename",
      "label": "Ant Renamer",
      "type": "external",
      "path": "apps/multi_rename/Renamer.exe",
      "args": ["-af", "${selectedFilesJoined}"],
      "refreshAfter": true
    },
    {
      "id": "unlockDelete",
      "label": "ThisIsMyFile",
      "type": "external",
      "path": "apps/delete/unlock_delete/ThisIsMyFile.exe",
      "args": ["${selectedFilesJoined}"],
      "refreshAfter": true
    },
    {
      "id": "wipeDelete",
      "label": "SDelete",
      "type": "external",
      "path": "apps/delete/wipe/sdelete64.exe",
      "args": ["/accepteula", "${selectedFilesJoined}"],
      "refreshAfter": true
    },
    {
      "id": "help",
      "label": "Help",
      "shortcut": "F1",
      "aliases": ["manual", "docs"],
      "contexts": ["global", "commandPalette"],
      "selection": "none",
      "type": "builtin"
    },
    {
      "id": "rename",
      "label": "Rename",
      "shortcut": "F2",
      "aliases": ["mvname"],
      "contexts": ["filePane", "commandPalette"],
      "selection": "single",
      "type": "builtin"
    },
    {
      "id": "renameShift",
      "label": "Rename",
      "shortcut": "Shift+F6",
      "contexts": ["filePane"],
      "selection": "single",
      "type": "builtin",
      "builtin": "rename"
    },
    {
      "id": "view",
      "label": "View",
      "shortcut": "F3",
      "aliases": ["preview"],
      "contexts": ["filePane", "commandPalette"],
      "selection": "single",
      "type": "builtin",
      "path": "apps/view/UniversalViewer/Viewer.exe",
      "args": ["${selectedFile}"]
    },
    {
      "id": "edit",
      "label": "Edit",
      "shortcut": "F4",
      "aliases": ["notepad"],
      "contexts": ["filePane", "commandPalette"],
      "selection": "single",
      "type": "builtin",
      "path": "apps/edit/Notepad4.exe",
      "args": ["${selectedFile}"]
    },
