      "selection": "any",
      "type": "builtin",
      "builtin": "deleteWipe"
    },
    {
      "id": "deleteWipeKey",
      "label": "Delete & Wipe",
      "shortcut": "Shift+Delete",
      "contexts": ["filePane"],
      "selection": "any",
      "type": "builtin",
      "builtin": "deleteWipe"
    },
    {
      "id": "terminal",
      "label": "Open Terminal Here",
      "shortcut": "F9",
      "aliases": ["shell", "cmd", "powershell"],
      "contexts": ["global", "commandPalette"],
      "selection": "none",
      "type": "builtin"
    },
    {
      "id": "explorer",
      "label": "Open Explorer Here",
      "shortcut": "Alt+F9",
      "aliases": ["open folder"],
      "contexts": ["global", "commandPalette"],
      "selection": "none",
      "type": "builtin"
    },
    {
      "id": "search",
      "label": "Search Files",
      "shortcut": "F10",
      "aliases": ["find", "grep"],
      "contexts": ["global", "commandPalette"],
      "selection": "none",
      "type": "builtin"
    },
    {
      "id": "searchAlt",
      "label": "Search Files",
      "shortcut": "Ctrl+F",
      "contexts": ["global"],
      "selection": "none",
      "type": "builtin",
      "builtin": "search"
    },
    {
      "id": "pack",
      "label": "Pack to Zip",
      "shortcut": "F11",
      "aliases": ["zip", "archive"],
      "contexts": ["filePane", "commandPalette"],
      "selection": "any",
      "type": "builtin",
      "path": "apps/pack_unpack/7zG.exe",
      "args": ["a", "${archiveFile}", "${selectedFilesJoined}"],
      "refreshAfter": true
    },
    {
      "id": "unpack",
      "label": "Unpack",
      "shortcut": "F12",
      "aliases": ["extract", "unzip"],
      "contexts": ["filePane", "commandPalette"],
      "selection": "any",
      "type": "builtin",
      "path": "apps/pack_unpack/7zG.exe",
      "args": ["x", "-o${destinationPath}", "${selectedFile}"],
      "refreshAfter": true
    },
    {
      "id": "extractAll",
      "label": "Extract All",
      "shortcut": "Alt+F12",
      "aliases": ["force extract"],
      "contexts": ["global", "commandPalette"],
      "selection": "any",
      "type": "builtin",
      "path": "apps/extract_all/UniExtract/UniExtract.exe",
      "args": ["${selectedFile}", "${destinationPath}"],
      "refreshAfter": true
    },
    {
      "id": "mergePdf",
      "label": "Merge PDF Files",
      "shortcut": "Shift+F1",
      "aliases": ["pdf merge"],
      "contexts": ["global", "commandPalette"],
      "selection": "multi",
      "type": "builtin",
      "path": "apps/pdf/pdftk.exe",
      "args": ["${selectedFilesJoined}", "cat", "output", "${outputPdf}"],
      "refreshAfter": true
    },
    {
      "id": "extractPdfPages",
      "label": "Extract PDF Pages",
      "shortcut": "Shift+F2",
      "aliases": ["pdf split"],
      "contexts": ["global", "commandPalette"],
      "selection": "singleFile",
      "type": "builtin",
      "path": "apps/pdf/pdftk.exe",
      "args": ["${selectedFile}", "burst", "output", "${outputPattern}"],
      "refreshAfter": true
    },
    {
      "id": "refresh",
      "label": "Refresh Panels",
      "shortcut": "Ctrl+R",
      "aliases": ["reload"],
      "contexts": ["global", "commandPalette"],
      "selection": "none",
      "type": "builtin"
    },
    {
      "id": "openCommandPalette",
