      "shortcut": "Ctrl+Shift+P",
      "contexts": ["global"],
      "selection": "none",
      "type": "builtin",
      "builtin": "openCommandPalette"
    },
    {
      "id": "leftPathCombo",
      "label": "Left Path",
      "shortcut": "Alt+F1",
      "contexts": ["global"],
      "selection": "none",
      "type": "builtin",
      "builtin": "leftPathCombo"
    },
    {
      "id": "rightPathCombo",
      "label": "Right Path",
      "shortcut": "Alt+F2",
      "contexts": ["global"],
      "selection": "none",
      "type": "builtin",
      "builtin": "rightPathCombo"
    }
