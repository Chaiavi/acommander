      "id": "copy",
      "label": "Copy",
      "shortcut": "F5",
      "aliases": ["cp"],
      "contexts": ["filePane", "commandPalette"],
      "selection": "single",
      "type": "builtin",
      "path": "apps/copy/FastCopy.exe",
      "args": ["/cmd=diff", "/auto_close", "${selectedFile}", "/to=${targetFolder}"],
      "refreshAfter": true
    },
    {
      "id": "move",
      "label": "Move",
      "shortcut": "F6",
      "aliases": ["mv"],
      "contexts": ["filePane", "commandPalette"],
      "selection": "single",
      "type": "builtin",
      "path": "apps/copy/FastCopy.exe",
      "args": ["/cmd=move", "/auto_close", "${selectedFile}", "/to=${targetFolder}"],
      "refreshAfter": true
    },
    {
      "id": "mkdir",
      "label": "Create Directory",
      "shortcut": "F7",
