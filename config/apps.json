      "contexts": ["filePane", "commandPalette"],
      "selection": "none",
      "type": "builtin"
    },
    {
      "id": "mkfile",
      "label": "Create File",
      "shortcut": "Alt+F7",
      "aliases": ["new file", "touch"],
      "contexts": ["global", "commandPalette"],
      "selection": "none",
      "type": "builtin"
    },
    {
      "id": "delete",
      "label": "Delete",
      "shortcut": "F8",
      "aliases": ["remove", "rm", "del"],
      "contexts": ["filePane", "commandPalette"],
      "selection": "any",
      "type": "builtin"
    },
    {
      "id": "deleteKey",
      "label": "Delete",
